 {
  "name": "n8n-nodes-sms",
  "version": "0.1.0",
  "description": "n8n community node for SMS API integration",
  
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "oneman",
    "email": "onemanarmy1032@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sid/n8n-nodes-sms.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier Sms/**/*.ts credentials/**/*.ts --write",
    "lint": "eslint Sms/**/*.ts credentials/**/*.ts package.json",
   "lintfix": "eslint Sms/**/*.ts credentials/**/*.ts package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js Sms/**/*.ts credentials/**/*.ts package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
  "n8nNodesApiVersion": 1,
  "credentials": [
    "dist/credentials/SmsApi.credentials.js"
  ],
  "nodes": [
    "dist/Sms/sms.node.js"
  ]
},
  "devDependencies": {
    "@typescript-eslint/parser": "~5.45",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "typescript": "~4.8.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}