{
  "name": "n8n-ivrapi-community-nodes",
  "version": "0.1.0",
  "description": "n8n community node for ivrapi API integration",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://github.com/advicospoland-ai/n8n-ivrapi-community-nodes#readme",
  "author": {
    "name": "oneman",
    "email": "onemanarmy1032@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/advicospoland-ai/n8n-ivrapi-community-nodes.git"
  },
  
  "main": "index.js",
  "scripts": {
    "build": "tsc && cpx \"ivrapi/*.svg\" dist/ivrapi",
    "dev": "tsc --watch",
    "format": "prettier ivrapi/**/*.ts credentials/**/*.ts --write",
    "lint": "eslint ivrapi/**/*.ts credentials/**/*.ts package.json",
    "lintfix": "eslint ivrapi/**/*.ts credentials/**/*.ts package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js ivrapi/**/*.ts credentials/**/*.ts package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ivrapiApi.credentials.js"
    ],
    "nodes": [
      "dist/ivrapi/ivrapi.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "~5.45",
    "cpx": "^1.5.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "typescript": "~4.8.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
